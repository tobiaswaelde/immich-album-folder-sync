  "scripts": {
    "build": "nest build",
    "bundle": "rimraf build && ncc build dist/main.js --out build --minify",
    "postbuild": "yarn run bundle",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
